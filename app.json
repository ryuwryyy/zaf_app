{
  "expo": {
    "name": "odza",
    "slug": "odza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/design1.png",
    "scheme": "odza",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.odza.app",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.odza.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/design1.png",
        "backgroundImage": "./assets/images/design1.png",
        "monochromeImage": "./assets/images/design1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/design1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "プロフィール用の画像を選択するために、フォトライブラリへのアクセスを許可してください。"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/design1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f75f4243-74a5-4f6c-a105-cea0c7eb5a2b"
      }
    }
  }
}
